calc()
	if current-property
		for prefix in vendors
			arguments = unquote(arguments)
			add-property(current-property[0], s('-%s-calc(%s)', prefix, arguments))
		s('calc()', arguments)
	else
		error('calc() must be used within a property')

.navbar
	.brand
		margin 0
		padding 0

	.navbar-form
		line-height 48px
		margin-right 10px
		input, button
			margin 0

	&.navbar-fixed-top
		position static

.container-fluid
	height calc('100% - 49px')
	padding 0
html
	height: 100%
body
	height: 100%
	margin: 0
	padding: 0
#map_canvas
	height: 100%
